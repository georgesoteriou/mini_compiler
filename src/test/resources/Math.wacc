begin
    int Math_pow(int a, int b) is
        int result = 1;
        for int i = 0; i < b; i++ do
            result *= a;
        end;
        return result;
    end

    int Math_factorial(int a) is
        int result = 1;
        for int i = 1; i <= a; i++ do
            result *= i;
        end;
        return result;
    end

end